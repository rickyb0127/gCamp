<div class = "page-header">
  <h1>Projects <%= link_to "New Project", new_project_path(@project), class: "btn btn-info pull-right" %></h1>
</div>

<table class="table">

  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

    <tbody>
      <tr>
        <% @projects.each do |project| %>
        <td><%= link_to project.name, project_path(project) %></td>
      </tr>
        <% end %>
    </tbody>

</table>
