<div class="page-header">
  <h1>Edit Task</h1>
</div>

<ol class="breadcrumb">
  <li><%= link_to "Projects", projects_path %></li>
  <li><%= link_to @project.name, project_path(@project) %></li>
  <li><%= link_to "Tasks", project_tasks_path(@project) %></li>
  <li><%= link_to @task.description, project_task_path %></li>
  <li class="active">Edit <%= @task.description %></li>
</ol>

<%= render partial: "form", locals: {button_name: "Update Task"} %>
